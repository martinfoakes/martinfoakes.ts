{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "prettier": true,
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false,
      "import-name": false,
      "no-boolean-literal-compare": false
    },
    "rulesDirectory": []
}